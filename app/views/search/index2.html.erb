<div class="spsearch">
    <div class="spserch-form">
        <%= form_with url: search_result_path, method: :get, local: true do |f| %>
            <%= f.text_field :q, placeholder: "タグワードを入力" %>
            <%= f.submit "検索" %>
        <% end %>
    </div>

    <div class="spsearch-tag">
        <% @tags.each do |tag| %>
        <ul>
            <li><%= tag.tag_name %></li>
        </ul>
        <% end %>
    </div>
</div>